@Article{	  vaquero-sigcom11,
  title		= {Dynamically Scaling Applications in the Cloud},
  author	= {Vaquero, Luis M. and Rodero-Merino, Luis and Buyya,
		  Rajkumar},
  journal	= {{SIGCOMM} Comput. Commun. Rev.},
  year		= {2011},
  month		= jan,
  number	= {1},
  pages		= {45--52},
  volume	= {41},
  abstract	= {Scalability is said to be one of the major advantages
		  brought by the cloud paradigm and, more specifically, the
		  one that makes it different to an "advanced outsourcing"
		  solution. However, there are some important pending issues
		  before making the dreamed automated scaling for
		  applications come true. In this paper, the most notable
		  initiatives towards whole application scalability in cloud
		  environments are presented. We present relevant efforts at
		  the edge of state of the art technology, providing an
		  encompassing overview of the trends they each follow. We
		  also highlight pending challenges that will likely be
		  addressed in new research efforts and present an ideal
		  scalable cloud system.},
  doi		= {10.1145/1925861.1925869},
  issn		= {0146-4833},
  keywords	= {Cloud computing, scalability},
